    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "helmet": "^3.16.0",
    "html-to-text": "^5.1.1",
    "jimp": "^0.6.4",
    "juice": "^5.2.0",
    "marked": "^0.6.2",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "mongodb-backup": "^1.6.9",
    "mongodb-restore": "^1.6.2",
    "mongoose": "^5.5.5",
    "mongoose-mongodb-errors": "^0.0.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "next-day": "^1.1.0",
    "nodemailer": "^6.1.1",
    "ol": "^5.3.2",
    "passport": "^0.4.0",
