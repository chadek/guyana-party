{
  "name": "guyana-party",
  "description": "site d'évènements en guyane",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack-dev-server --open --config webpack.dev.js",
    "start": "webpack -p --progress --config webpack.prod.js && node ./start.js",
    "assets": "webpack -w --config webpack.dev.js --display-max-modules 0",
    "dev": "concurrently \"npm run assets\" \"node ./start.js\" --names \"webpack,node   \" --prefix name",
    "sample": "node ./data/load-sample-data.js",
    "blowitallaway": "node ./data/load-sample-data.js --delete",
    "backup": "node ./data/load-sample-data.js --backup",
    "restore": "node ./data/load-sample-data.js --restore",
    "lint": "eslint --ignore-pattern '/node_modules/*' --ignore-pattern '**/dist/*' --ignore-pattern '**/vendor/*' -o ./eslint_result.html -f html ."
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
