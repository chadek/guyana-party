{
  "name": "guyana-party",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 8.9.1"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack-dev-server --open --config webpack.dev.js",
    "start": "webpack -p --progress --config webpack.prod.js && node ./start.js",
    "assets": "webpack -w --config webpack.dev.js --display-max-modules 0",
    "dev": "concurrently \"npm run assets\" \"node ./start.js\" --names \"webpack,node   \" --prefix name",
    "linter": "standard --parser babel-eslint",
    "test": "snyk test",
    "sample": "node ./data/load-sample-data.js",
    "blowitallaway": "node ./data/load-sample-data.js --delete"
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dompurify": "^1.0.8",
    "dotenv": "^6.1.0",
    "es6-promisify": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "helmet": "^3.14.0",
    "html-to-text": "^4.0.0",
    "jimp": "^0.5.3",
    "juice": "^4.3.2",
    "marked": "^0.5.1",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mongoose": "^5.3.2",
    "mongoose-mongodb-errors": "^0.0.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.0",
    "nodemailer": "^4.6.8",
    "ol": "^5.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "pug": "^2.0.3",
    "sanitize": "2.1.0",
