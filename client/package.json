{
  "name": "guyana-party-client",
  "description": "Site d'évènements en Guyane",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --watch",
    "test:cov": "jest --watchAll --coverage",
    "lint": "eslint --ignore-pattern '/public/*' --ignore-pattern '.cache/*' -o ./eslint_result.html -f html ."
  },
  "engines": {
    "node": "12.15.x"
  },
  "dependencies": {
    "@date-io/core": "2.4.0",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.9.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "@reach/router": "1.3.1",
    "axios": "0.19.2",
    "babel-jest": "25.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "chai": "4.2.0",
    "chai-enzyme": "1.0.0-beta.1",
    "chai-jest-diff": "1.0.2",
    "chai-jest-snapshot": "2.0.0",
    "client-compress": "2.1.2",
