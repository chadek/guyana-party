    "react-dom": "16.12.0",
    "react-dropzone": "10.2.1",
    "react-facebook-login": "4.1.1",
    "react-google-login": "5.1.0",
    "react-helmet": "5.2.1",
    "react-leaflet": "2.6.1",
    "react-leaflet-markercluster": "1.1.8",
    "react-mde": "8.0.2",
    "react-slick": "0.25.2",
    "showdown": "1.9.1",
    "sinon-chai": "3.4.0",
    "slick-carousel": "1.8.1",
    "styled-components": "5.0.1",
    "swr": "0.1.16",
    "validator": "12.2.0",
    "waait": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@testing-library/react-hooks": "3.2.1",
    "babel-eslint": "9.0.0",
    "babel-preset-gatsby": "0.2.29",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "sinon": "8.1.1"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "lf",
          "printWidth": 120,
          "semi": false,
          "singleQuote": true,
          "trailingComma": "none",
          "jsxSingleQuote": true
        }
      ],
      "jsx-a11y/label-has-for": 0,
      "react/jsx-sort-props": "warn"
    }
  },
  "author": "Christopher Servius <https://github.com/spidergon>",
  "repository": {
    "type": "git",
    "url": "https://github.com/spidergon/guyana-party-client"
  },
  "bugs": {
    "url": "https://github.com/spidergon/guyana-party-client/issues"
  }
}
