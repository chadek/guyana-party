{
  "name": "guyana-party-server",
  "version": "1.0.0",
  "description": "site d'Ã©vÃ¨nements en guyane",
  "license": "MIT",
  "private": true,
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "server": "nodemon start.js --ignore tests/ --ignore client_build/ --ignore uploads/",
    "client": "yarn --cwd ../client start",
    "client:build": "rm -rf client_build && yarn --cwd ../client clean && yarn --cwd ../client build && cp -r ../client/public client_build",
    "dev": "clear; concurrently \"yarn server\" \"yarn client\" --names \"ðŸ¤–server,ðŸ¤“client\" -c \"bgBlue.bold,bgMagenta.bold\"",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --watchAll --coverage",
    "pretest": "cross-env NODE_ENV=test babel-node ./src/core/tools/pretest.js",
    "lint": "eslint -o ./eslint_result.html -f html .",
    "lint:fix": "eslint . --fix"
  },
  "engines": {
    "node": "12.16.x",
    "yarn": "1.22.x"
  },
  "dependencies": {
    "@babel/register": "7.8.6",
    "bcrypt": "4.0.0",
    "cloudinary": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.1.0",
    "connect-mongo": "3.2.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "google-auth-library": "5.10.1",
    "helmet": "3.21.3",
    "html-to-text": "5.1.1",
    "jimp": "0.9.3",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.9.2",
    "mongoose-unique-validator": "2.0.3",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "nodemailer": "6.4.3",
    "sanitize": "2.1.0",
    "slugify": "1.3.6",
    "uuidv4": "6.0.5",
    "validator": "12.2.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/node": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
