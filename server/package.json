    "@babel/preset-env": "7.8.6",
    "@babel/runtime": "7.8.4",
    "babel-eslint": "9.0.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "cross-env": "7.0.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "webpack": "4.41.6"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "lf",
          "printWidth": 120,
          "semi": false,
          "singleQuote": true,
          "trailingComma": "none"
        }
      ]
    }
  },
  "repository": {
